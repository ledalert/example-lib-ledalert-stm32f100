#We are using a local lib-ledalert-stm32f100
LIB_LEDALERT_STM32F100_LOCATION = lib/lib-ledalert-stm32f100/

ifeq ($(origin LIB_LEDALERT_STM32F100_LOCATION), undefined)
$(error LIB_LEDALERT_STM32F100_LOCATION is not specified!)
endif


LIB_LEDALERT_STM32F100_INCLUDES = $(LIB_LEDALERT_STM32F100_LOCATION)inc
LIB_LEDALERT_STM32F100_LIB = $(LIB_LEDALERT_STM32F100_LOCATION)build/lib_ledalert_stm32f100.a
LIB_INCLUDES += $(LIB_LEDALERT_STM32F100_LOCATION)build/
LIB_NAMES += _ledalert_stm32f100


$(LIB_LEDALERT_STM32F100_LIB):
	$(MAKE) -C $(LIB_LEDALERT_STM32F100_LOCATION)